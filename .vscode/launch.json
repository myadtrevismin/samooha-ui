    "version": "0.2.0",
    "configurations": [
      {
        "name": "Attach to iOS on device",
        "type": "cordova",
        "request": "attach",
        "platform": "ios",
        "target": "device",
        "cwd": "${workspaceFolder}",
        "sourceMaps": true
      },
    {
      "name": "Run iOS on device",
      "type": "cordova",
      "request": "launch",
      "platform": "ios",
      "target": "",
      "sourceMaps": true,
      "cwd": "${workspaceFolder}"
    },
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:4202",
            "webRoot": "${workspaceFolder}"
        }
    ]
}
